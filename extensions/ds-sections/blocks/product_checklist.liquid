<style>

   .info-container-v5 {
       display: flex;
       flex-direction: column;
       align-items: flex-start;
       gap: 8px;
       font-size: {{block.settings.checklist-font-size}}px;
       font-weight: 500;
       color: {{block.settings.checklict-color}};
       margin-top: -10px;
       margin-bottom: 0px;
   }

   .info-item-v5 {
       transition: transform 0.3s ease;
       display: flex;
       align-items: center;
       gap: 6px;
       background: {{block.settings.checklict-bg}};
       padding: 6px 10px;
       border-radius: 8px;
   }

   .info-item-v5:hover {
       transform: translateX(10px);
   }

   .info-item {
       font-size: {{block.settings.checklist-font-size}}px;
       color: {{block.settings.checklict-color}};
   }

</style>

